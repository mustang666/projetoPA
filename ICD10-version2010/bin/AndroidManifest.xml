<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.icd10_version2010"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PesquisarActivity"
            android:label="@string/title_activity_pesquisar"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.icd10_version2010.MainActivity" />
        </activity>
        <activity
            android:name=".FavoritosActivity"
            android:label="@string/title_activity_favoritos"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.icd10_version2010.MainActivity" />
        </activity>
        <activity
            android:name=".EstatisticasActivity"
            android:label="@string/title_activity_estatisticas"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.icd10_version2010.MainActivity" />
        </activity>
        <activity
            android:name=".HomeActivity"
            android:label="@string/title_activity_home"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.icd10_version2010.MainActivity" />
        </activity>
        <activity
            android:name=".QuestoesActivity"
            android:label="@string/title_activity_questoes"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.icd10_version2010.MainActivity" />
        </activity>
        <activity
            android:name=".TrainingActivity"
            android:label="@string/title_activity_training"
            android:parentActivityName=".QuestoesActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.icd10_version2010.QuestoesActivity" />
        </activity>
        <activity
            android:name=".EstatQuestActivity"
