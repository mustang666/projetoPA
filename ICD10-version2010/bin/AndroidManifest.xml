                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.icd10_version2010.QuestoesActivity" />
        </activity>
        <activity
            android:name=".HistoricoQuestActivity"
            android:label="@string/title_activity_historico_quest"
            android:parentActivityName=".QuestoesActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.icd10_version2010.QuestoesActivity" />
        </activity>
        <activity
            android:name=".AlertActivity"
            android:label="@string/title_activity_alert" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
